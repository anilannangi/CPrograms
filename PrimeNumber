#include<stdio.h>
int prime(int);
int main()
{
int n,res=0;
printf("enter a number");
scanf("%d",&n);
res=prime(n);
if(res==0)
{
printf("\n%d is a prime number",n);
}
else
{
printf("\n%d is not a prime number",n);
}
return 0;
}
int prime (int i)
{
int c=0;
for(i=0;i<c;i++)
{
if(i%c==0)
{
c++;
}
}
return 0;
}
















